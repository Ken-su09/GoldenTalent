<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/splashScreenFragment">

    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.suonk.goldentalent.ui.main.SplashScreenFragment"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreenFragment_to_firstLaunchFragment"
            app:destination="@id/firstLaunchFragment" />
    </fragment>
    <fragment
        android:id="@+id/firstLaunchFragment"
        android:name="com.suonk.goldentalent.ui.auth.FirstLaunchFragment"
        android:label="fragment_first_launch"
        tools:layout="@layout/fragment_first_launch" />
